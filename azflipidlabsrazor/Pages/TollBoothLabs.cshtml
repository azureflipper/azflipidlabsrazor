@page
@model TollBoothLabsModel

<div class="row">
    <img src="~/images/meetup3.png" />
</div>
<div class="row">
    <form method="post" enctype="multipart/form-data" action="/api/FileUpload/SaveFile">
        <div class="form-group">
            <div class="col-md-10">
                <h3>Upload license plate:</h3>
                <label for="upload-photo" style="cursor:pointer;width:140px;height:40px;border:dashed;border-color:#1d1d1d;line-height:35px;font-size:20px;font-family:'Segoe UI';text-align:center;vertical-align:middle" >Browse</label>
                <input type="file" name="files" id="upload-photo" style="opacity:0;position:absolute" />
            </div>
        </div>
        <div class="form-group">
            <div class="col-md-10">
                <br />
                <input type="submit" value="Upload >" style="width:140px;height:40px;background-color:#89c402;color:#1a1a1a;font-size:20px;font-family:'Segoe UI';border:none" />
            </div>
        </div>
    </form>
</div>

<br />
<div class="row">
    <div class="col-md-10">
        <h3>License Plates:</h3>
    </div>
</div>


